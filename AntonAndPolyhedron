#include<iostream>
using namespace std;
int main(){
string s1;
int i,n;
string s2="Tetrahedron";
string s3="Cube";
string s4="Octahedron";
string s5="Dodecahedron";
string s6="Icosahedron";

cin>>n;
int s=0;
for(i=0;i<n;i++){
cin>>s1;
if(s1==s2){
  s=s+4;
}
if(s1==s3){
  s=s+6;
}
if(s1==s4){
  s=s+8;
}
if(s1==s5){
  s=s+12;
}
if(s1==s6){
  s=s+20;
}
}
cout<<s<<endl;

return 0;
}
